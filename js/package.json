{
  "name": "nina-workspace",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "nina-common",
    "nina-radio",
    "soft-lp",
    "web"
  ],
  "scripts": {
    "nina-common": "yarn workspace nina-common",
    "nina-radio": "yarn workspace nina-radio",
    "nina-radio:build": "yarn nina-common build && yarn nina-radio build",
    "soft-lp": "yarn workspace soft-lp",
    "soft-lp:build": "yarn nina-common build && yarn soft-lp build",
    "web": "yarn workspace web",
    "web:build": "yarn nina-common build && yarn web build"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.4",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "ansi-regex": "^5.0.1",
    "glob-parent": "^5.1.2",
    "next": "^12.0.9",
    "nanoid": "^3.1.31",
    "node-fetch": "^2.6.7",
    "node-forge": "^1.0.0",
    "react-animated-ellipsis": "^1.0.0",
    "styled-components": "^5.3.3"
  },
  "browser": {
    "crypto": false
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  }
}
